# -*- mode: snippet -*-
# name: ndc_method
# --
    sii_int ret;

    if (in_argc == 0) {
        out_result->$1.fType = FNDCValue_TYPE_$2;
    } else {
        ret = sii_ac_strcmp(in_argv[0], SII_T("$3"));
        out_result->uBool.fType = FNDCValue_TYPE_BOOL;
        out_result->uBool.fBool = (ret == 0);
    }
    return SII_E_OK;
